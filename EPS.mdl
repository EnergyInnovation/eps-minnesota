Boolean Is This Plant Type Eligible for Replacement of Retired Plants[Electricity Source\
		]=
	IF THEN ELSE(
	
	  BPaFF Boolean Is This Plant Type a Peaker[Electricity Source] = 0
	  :AND:
	  Difference in Cost Relative to Mean by Source[Electricity Source,newly built] < 0,
	
	  1,
	
	  0
	)
	~	Dmnl
	~		|

Boolean Plant Types that May Not be Built due to Having Been Retired to Zero[Electricity Source\
		]=
	IF THEN ELSE(
	  SUM(SYC Start Year Electricity Generation Capacity[Electricity Source,Power Plant Quality\
		!]) > 0,
	
	  IF THEN ELSE(
	    Retiring Generation Capacity[Electricity Source]
	    >= SUM(Last Year Electricity Generation Capacity[Electricity Source,Power Plant Quality\
		!]),
	
	    1,
	
	    0
	  ),
	
	  0
	)
	~	Dmnl
	~		|

