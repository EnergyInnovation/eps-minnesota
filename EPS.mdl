	BFPaT BAU Pretax Fuel Price by Sector[jet fuel or kerosene,transportation sector]
	+ BAU Transportation Fuel Total Tax Amount per Unit Energy[aircraft,jet fuel tf] ~~|
BAU Transportation Fuel Cost per Unit Energy[heavy or residual fuel oil tf]=
	BFPaT BAU Pretax Fuel Price by Sector[heavy or residual fuel oil,transportation sector\
		]
	+ BAU Transportation Fuel Total Tax Amount per Unit Energy[ships,heavy or residual fuel oil tf\
		] ~~|
BAU Transportation Fuel Cost per Unit Energy[LPG propane or butane tf]=
	BFPaT BAU Pretax Fuel Price by Sector[LPG propane or butane,transportation sector]
	+ BAU Transportation Fuel Total Tax Amount per Unit Energy[LDVs,LPG propane or butane tf\
		] ~~|
BAU Transportation Fuel Cost per Unit Energy[hydrogen tf]=
	BFPaT BAU Pretax Fuel Price by Sector[hydrogen,transportation sector]
	+ BAU Transportation Fuel Total Tax Amount per Unit Energy[LDVs,hydrogen tf]
	~	$/BTU
	~	We need this subscripted only by fuel type, not vehicle type, for use \
		calculating percentage differences from BAU.  Carbon intensity of fuels \
		should not vary significantly based on which type of vehicle burns those \
		fuels, as CO2 is not captured or eliminated through control technologies \
		in vehicles (like a catalytic converter for NOx).  Accordingly, we use the \
		main vehicle type that burns each fuel type to populate this variable.
	|

BAU Transportation Fuel Other Tax Amount per Unit Energy[Transportation Fuel]=
	BFPaT BAU Fuel Tax by Sector[transportation fuel within all fuels,transportation sector\
		]
	~	$/BTU
	~		|

BAU Transportation Fuel Taxable CO2e Emissions Intensity[Vehicle Type,Transportation Fuel\
		]=
	IF THEN ELSE(
	  BENCEfCT Boolean Exempt Non CO2 Emissions from Carbon Tax > 0,
	
	  Transportation Fuel Pollutant Emissions Intensities This Year[Vehicle Type,CO2,Transportation Fuel\
		]
	  * GWP by Pollutant[CO2],
	
	  SUM(Transportation Fuel Pollutant Emissions Intensities This Year[Vehicle Type,Pollutant\
		!,Transportation Fuel]
	  * GWP by Pollutant[Pollutant!])
	)
	~	g CO2e/BTU
	~		|

BAU Transportation Fuel Total Tax Amount per Unit Energy[Vehicle Type,Transportation Fuel\
		]=
	BAU Transportation Fuel Carbon Tax Amount per Unit Energy[Vehicle Type,Transportation Fuel\
		]
	+ BAU Transportation Fuel Other Tax Amount per Unit Energy[Transportation Fuel]
	~	$/BTU
	~		|

BAU Transportation Peak Hour Electricity Demand by Season[Vehicle Type,Cargo Type,Season\
		]=
	BAU Transportation Average Hourly Electricity Demand[Vehicle Type,Cargo Type]
	* ELF Equipment Load Factors for Vehicles[Vehicle Type,Season]
	~	MW
	~		|

BAU Transportation Pollutant Emissions Disaggregated[Vehicle Type,Pollutant,Cargo Type\
		,Transportation Fuel]=
	SUM(BAU Transportation Sector Fuel Used[Vehicle Type,Cargo Type,Vehicle Technology!,\
		Transportation Fuel])
	  * Transportation Fuel Pollutant Emissions Intensities This Year[Vehicle Type,Pollutant\
		,Transportation Fuel]
	~	g pollutant
	~		|

BAU Transportation Sector Carbon Tax Revenue=
	SUM(BAU Transportation Sector Fuel Used[Vehicle Type!,Cargo Type!,Vehicle Technology\
		!,Transportation Fuel!]
	* BAU Transportation Fuel Carbon Tax Amount per Unit Energy[Vehicle Type!,Transportation Fuel\
		!])
	~	$
	~		|

